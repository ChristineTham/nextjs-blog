{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Chris Tham",
    "email": "chris.tham@hellotham.com",
    "url": "https://hellotham.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristineTham/nextjs-blog.git"
  },
  "scripts": {
    "dev": "env NODE_OPTIONS=--max_old_space_size=8192 next dev",
    "build": "env NODE_OPTIONS=--max_old_space_size=8192 next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "eslint": "eslint . --ext ts --ext tsx --ext js",
    "lint": "next lint",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn format"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "semi": false
  },
  "dependencies": {
    "@headlessui/react": "^1.1.1",
    "@mailchimp/mailchimp_marketing": "^3.0.48",
    "@sendgrid/mail": "^7.4.5",
    "@silvenon/remark-smartypants": "^1.0.0",
    "@tailwindcss/typography": "^0.4.0",
    "date-fns": "^2.21.3",
    "g": "^2.0.1",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.0",
    "next": "^11.0.0",
    "next-mdx-remote": "^3.0.1",
    "next-seo": "^4.24.0",
    "prism-react-renderer": "^1.2.0",
    "react": "^17.0.2",
    "react-bnb-gallery": "^1.4.4",
    "react-dom": "^17.0.2",
    "react-image-lightbox": "^5.1.1",
    "react-live": "^2.2.3",
    "react-paginate": "^7.1.2",
    "react-photo-gallery": "^8.0.0",
    "react-typed": "^1.2.0",
    "rehype": "^11.0.0",
    "rehype-katex": "^5.0.0",
    "remark": "^13.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-code-titles": "^0.1.2",
    "remark-html": "^13.0.2",
    "remark-math": "3",
    "remark-slug": "^6.0.0",
    "remark-toc": "^7.2.0",
    "rss": "^1.2.2",
    "tailwindcss": "^2.1.2",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.5",
    "@types/react-paginate": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "@vercel/node": "^1.10.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^27.0.2",
    "eslint": "^7.26.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "globby": "^11.0.4",
    "husky": "^7.0.1",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "postcss": "^8.2.14",
    "prettier": "^2.3.1",
    "typescript": "^4.3.3"
  }
}
